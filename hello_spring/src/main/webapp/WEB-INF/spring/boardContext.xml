<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="boardDaoImpl" class="com.ktds.board.dao.BoardDaoImpl">
		<property name="dataSource" ref="dataSource"></property>
	</bean>
	
	<bean id="boardBizImpl" class="com.ktds.board.biz.BoardBizImpl">
		<property name="boardDao" ref="boardDaoImpl"></property>
	</bean>
	
	<bean id="boardServiceImpl" class="com.ktds.board.service.BoardServiceImpl">
		<property name="boardBiz" ref="boardBizImpl"></property>
	</bean>


</beans>
